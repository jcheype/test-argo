{
  "name": "test-argo",
  "version": "1.0.0",
  "main": "dist/server.cjs",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --minify --platform=node --target=node18 --format=cjs --outfile=dist/server.cjs",
    "start": "node dist/server.cjs",
    "dev": "tsx watch src/server.ts",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trpc/server": "^11.6.0",
    "@types/express": "^4.17.21",
    "express": "^4.19.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@trpc/client": "^11.6.0",
    "node-fetch": "^3.3.2",
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^20.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
